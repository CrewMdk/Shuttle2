<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- The number of songs being edited with the tag (metadata) editor-->
    <plurals name="edit_tags_editing_count_songs">
        <item quantity="one">Edytujesz 
      <xliff:g example="1" id="count">{count}</xliff:g>
       utwór</item>
        <item quantity="other">Edycja 
      <xliff:g example="5" id="count">{count}</xliff:g>
       utworów</item>
        <item quantity="few">Edycja 
      <xliff:g example="5" id="count">{count}</xliff:g>
       utworów</item>
        <item quantity="many">Edycja 
      <xliff:g example="5" id="count">{count}</xliff:g>
       utworów</item>
    </plurals>
    <!-- The title of the 'edit tags' dialog. If 'tags' doesn't apply in your language, you could use 'metadata' or 'info' instead. -->
    <string name="edit_tags_action">Edytuj tagi</string>
    <!-- When editing tags, if multiple songs are being edited at the same time, this indicates that a field has multiple values.
    For example, editing two songs, which have two different artists. Editing the artist field will overwrite the artist for both songs. There are 'multiple values' for artist. -->
    <string name="edit_tags_multiple_values">Wiele wartości</string>
    <!-- Shown when the tag editor is in progress, reading tags (metadata) from the selected songs -->
    <string name="edit_tags_reading_tags">Odczytywanie tagów 
    <xliff:g example="1" id="progress">{progress}</xliff:g>
     / 
    <xliff:g example="1" id="count">{total}</xliff:g></string>
    <!-- Shown when the tag editor is in progress, writing tags (metadata) to the selected songs -->
    <string name="edit_tags_writing_tags">Odczytywanie tagów 
    <xliff:g example="1" id="progress">{progress}</xliff:g>
     / 
    <xliff:g example="1" id="count">{total}</xliff:g></string>
    <!-- Shown when the tag editor fails to read the tags (metadata) of a song -->
    <string name="edit_tags_read_failed">Nie można odczytać wybranych tagów utworów</string>
    <!-- Shown when the tag editor successfully writes the tags (metadata) of the songs -->
    <plurals name="edit_tags_success">
        <item quantity="one">Zaktualizowano 
      <xliff:g example="5" id="count">{count}</xliff:g>
       utwór</item>
        <item quantity="other">Zaktualizowano 
      <xliff:g example="5" id="count">{count}</xliff:g>
       utwory</item>
        <item quantity="few">Zaktualizowano 
      <xliff:g example="5" id="count">{count}</xliff:g>
       utwory</item>
        <item quantity="many">Zaktualizowano 
      <xliff:g example="5" id="count">{count}</xliff:g>
       utwory</item>
    </plurals>
    <!-- Shown when the tag editor fails to write the tags (metadata) to the songs-->
    <plurals name="edit_tags_failure">
        <item quantity="one">Nie udało się zaktualizować utworu</item>
        <item quantity="other">Nie udało się zaktualizować 
      <xliff:g example="5" id="count">{count}</xliff:g>
       z 
      <xliff:g example="5" id="total">{total}</xliff:g>
       utworów</item>
        <item quantity="few">Nie udało się zaktualizować 
      <xliff:g example="5" id="count">{count}</xliff:g>
       z 
      <xliff:g example="5" id="total">{total}</xliff:g>
       utworów</item>
        <item quantity="many">Nie udało się zaktualizować 
      <xliff:g example="5" id="count">{count}</xliff:g>
       z 
      <xliff:g example="5" id="total">{total}</xliff:g>
       utworów</item>
    </plurals>
    <!-- The hint (placeholder) text shown in the 'title' text field, when editing the tags (metadata) -->
    <string name="edit_tags_hint_title">Tytuł</string>
    <!-- The hint (placeholder) text shown in the 'artist' text field, when editing the tags (metadata) -->
    <string name="edit_tags_hint_artist">Artysta</string>
    <!-- The hint (placeholder) text shown in the 'album' text field, when editing the tags (metadata) -->
    <string name="edit_tags_hint_album">Album</string>
    <!-- The hint (placeholder) text shown in the 'album artist' text field, when editing the tags (metadata) -->
    <string name="edit_tags_hint_album_artist">Album artysty</string>
    <!-- The hint (placeholder) text shown in the 'year' text field, when editing the tags (metadata) -->
    <string name="edit_tags_hint_year">Rok</string>
    <!-- The hint (placeholder) text shown in the 'track' text field, when editing the tags (metadata) -->
    <string name="edit_tags_hint_track">Tor</string>
    <!-- The hint (placeholder) text shown in the 'track total' text field, when editing the tags (metadata) -->
    <string name="edit_tags_hint_track_total">Śledź sumę</string>
    <!-- The hint (placeholder) text shown in the 'disc' text field, when editing the tags (metadata) -->
    <string name="edit_tags_hint_disc">Dysk</string>
    <!-- The hint (placeholder) text shown in the 'disc total' text field, when editing the tags (metadata) -->
    <string name="edit_tags_hint_disc_total">Dysk łącznie</string>
    <!-- The hint (placeholder) text shown in the 'genres' text field, when editing the tags (metadata) -->
    <string name="edit_tags_hint_genres">Gatunki</string>
    <!-- The hint (placeholder) text shown in the 'lyrics' text field, when editing the tags (metadata) -->
    <string name="edit_tags_hint_lyrics">Tekst piosenki</string>
</resources>
