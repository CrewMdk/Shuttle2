<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of sleep timer dialog -->
    <string name="sleep_timer_dialog_title">睡眠计时器</string>
    <!-- Shown when the sleep timer has ended, but playback won't be paused until the current track ends -->
    <string name="sleep_timer_waiting_track_end">等待歌曲结束……</string>
    <!-- Sleep timer option to wait for a song to end before pausing playback -->
    <string name="sleep_timer_play_to_track_end">直到最后一首歌结束</string>
    <!-- Button to close the sleep timer dialog -->
    <string name="sleep_timer_dialog_button_close">关闭</string>
    <!-- Button to stop the sleep timer -->
    <string name="sleep_timer_dialog_button_stop_timer">关闭计时器</string>
    <!-- Button to set the time for the sleep timer -->
    <string name="sleep_timer_dialog_button_set_time">设定时间</string>
    <!-- The amount of time remaining before the sleep timer pauses playback -->
    <string name="sleep_timer_time_remaining">剩余 <xliff:g example="00:05:00" id="time">{time}</xliff:g></string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_5_minutes">5 分钟</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_15_minutes">15 分钟</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_30_minutes">30 分钟</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_1_hour">1 小时</string>
