<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of sleep timer dialog -->
    <string name="sleep_timer_dialog_title">Temporizador de apagado</string>
    <!-- Shown when the sleep timer has ended, but playback won't be paused until the current track ends -->
    <string name="sleep_timer_waiting_track_end">Esperando a que finalice la pista…</string>
    <!-- Sleep timer option to wait for a song to end before pausing playback -->
    <string name="sleep_timer_play_to_track_end">Reproducir la última canción para terminar</string>
    <!-- Button to close the sleep timer dialog -->
    <string name="sleep_timer_dialog_button_close">Cerrar</string>
    <!-- Button to stop the sleep timer -->
    <string name="sleep_timer_dialog_button_stop_timer">Detener temporizador</string>
    <!-- Button to set the time for the sleep timer -->
    <string name="sleep_timer_dialog_button_set_time">Establecer tiempo</string>
    <!-- The amount of time remaining before the sleep timer pauses playback -->
    <string name="sleep_timer_time_remaining"><xliff:g example="00:05:00" id="time">{time}</xliff:g> restante</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_5_minutes">5 minutos</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_15_minutes">15 minutos</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_30_minutes">30 minutos</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_1_hour">1 hora</string>
