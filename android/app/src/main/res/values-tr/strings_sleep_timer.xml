<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of sleep timer dialog -->
    <string name="sleep_timer_dialog_title">Uyku modu</string>
    <!-- Shown when the sleep timer has ended, but playback won't be paused until the current track ends -->
    <string name="sleep_timer_waiting_track_end">Şarkının bitmesin bekleniyor...</string>
    <!-- Sleep timer option to wait for a song to end before pausing playback -->
    <string name="sleep_timer_play_to_track_end">Bitirmek için son şarkıyı çal</string>
    <!-- Button to close the sleep timer dialog -->
    <string name="sleep_timer_dialog_button_close">Kapat</string>
    <!-- Button to stop the sleep timer -->
    <string name="sleep_timer_dialog_button_stop_timer">Zamanlayıcıyı durdur</string>
    <!-- Button to set the time for the sleep timer -->
    <string name="sleep_timer_dialog_button_set_time">Uyku zamanını ayarla</string>
    <!-- The amount of time remaining before the sleep timer pauses playback -->
    <string name="sleep_timer_time_remaining"><xliff:g example="00:05:00" id="time">{time}</xliff:g> kaldı</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_5_minutes">5 dakika</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_15_minutes">15 dakika</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_30_minutes">30 dakika</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_1_hour">1 saat</string>
</resources>
