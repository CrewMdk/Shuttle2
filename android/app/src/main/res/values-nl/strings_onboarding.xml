<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title of 'media directory selection' screen. User can choose to add directories to be scanned -->
    <string name="onboarding_directories_dialog_add_title">Mediamappen</string>
    <!-- Button to allow the user to add a directory, when using the 'S2 Media Scanner' -->
    <string name="onboarding_directories_dialog_add_button">Map toevoegen</string>
    <!-- Error message title shown when the user is missing the 'document provider' app. (An app that allows them to choose files & directories) -->
    <string name="onboarding_directories_dialog_missing_title">Ontbrekende documentprovider</string>
    <!-- Error message description shown when the user is missing the 'document provider' app. (An app that allows them to choose files & directories) -->
    <string name="onboarding_directories_dialog_missing_subtitle">Kan geen bestandsbeheerder vinden om mappen toe te voegen. Mogelijk moet je er een installeren of terugkeren naar het gebruik van de Android Media Store.</string>
    <!-- Explanation shown to user when they're about to choose which directories wil lbe scanned bv the S2 media scanner-->
    <string name="onboarding_directories_add_directories_help_text">Om naar muziek te kunnen scannen, heeft S2 toegang nodig tot je mediamappen. Voeg een map toe via de Android \'Documentprovider\' app.</string>
    <!-- Shown when the app is scanning the user's directories for media -->
    <string name="onboarding_directories_scanning">Aan het scannen…</string>
    <!-- Shown when the media scanner is scanning the user's directories for media-->
    <plurals name="onboarding_directories_scan_progress">
        <item quantity="one"><xliff:g example="1" id="count">{count}</xliff:g>
       audiobestand</item>
        <item quantity="other"><xliff:g example="30" id="count">{count}</xliff:g>
       audiobestanden</item>
    </plurals>
    <!-- Dialog title shown to user to indicate that S2 requires permission to access their storage -->
    <string name="onboarding_permission_dialog_title">Opslagtoestemming vereist</string>
    <!-- Dialog message shown to explain that S2 requires permission to access their storage-->
    <string name="onboarding_permission_dialog_subtitle">De shuttle functioneert mogelijk niet naar behoren zonder toestemming om toegang te krijgen tot je muziek</string>
    <!-- Dialog button to grant permission to the app to access users storage-->
    <string name="onboarding_permission_button_grant">Toestemming geven</string>
    <!-- Shown after user initially denies access to storage -->
    <string name="onboarding_permission_help_text">Om je muziek te vinden, heeft Shuttle toestemming nodig voor het lezen van je mediaopslag</string>
    <!-- Title of S2's Media Scanner, the thing that scans files on the device to read audio files/tags -->
    <string name="onboarding_media_scanner_title">Mediascanner</string>
    <!-- Shown when the media scanner is scanning -->
    <string name="onboarding_media_scanner_scanning">Aan het scannen…</string>
    <!-- Button to initiate a media scanner scan-->
    <string name="onboarding_media_scan">Scannen</string>
    <!-- Button to go back to the previous page, when onboarding -->
    <string name="onboarding_button_back">Terug</string>
    <!-- Button to go to the next page, when onboarding -->
    <string name="onboarding_button_next">Volgende</string>
    <!-- Button to trigger the MediaScanner to perform a rescan of audio files on the device-->
    <string name="onboarding_media_scanner_rescan">Opnieuw scannen</string>
    <!-- Shown when the S2 Media Provider is scanning song tags (metadata)-->
    <string name="onboarding_scanner_reading_song_tags">Tags lezen…</string>
    <!-- Shown when onboarding. Describes why S2 would like to use Firebase Analytics-->
    <string name="onboarding_data_collection_title">S2 zou graag je toestemming willen hebben voor het delen van anonieme crashgegevens en statistieken</string>
    <!-- Shown when onboarding. Switch to enable Crash Reporting -->
    <string name="onboarding_switch_crash_reporting">Firebase crashrapportage</string>
    <!-- Shown when onboarding. Describes what Crash Reporting is used for-->
    <string name="onboarding_switch_description_crash_reporting">Anonieme crashstatistieken die helpen bij het opsporen en oplossen van fouten</string>
    <!-- Shown when onboarding. Switch to enable Firebase Analytics -->
    <string name="onboarding_switch_firebase_analytics">Firebase statistieken</string>
    <!-- Shown when onboarding. Describes what Firebase Analytics is used for-->
    <string name="onboarding_switch_firebase_description_analytics">Schakelt het A/B-testen van functies in en houdt bij hoe gebruikers omgaan met de app</string>
    <!-- Shown during onboarding, or when choosing a media provider. -->
    <string name="onboarding_media_selection_subtitle">Kies waar je muziek vandaan komt. Kies voor Android Media Store als je het niet weet</string>
</resources>
