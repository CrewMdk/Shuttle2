    <string name="sleep_timer_time_remaining"><xliff:g example="00:05:00" id="time">{time}</xliff:g> resterend</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_5_minutes">5 minuten</string>
    <!-- Sleep timer time option-->
    <string name="sleep_timer_15_minutes">15 minuten</string>
