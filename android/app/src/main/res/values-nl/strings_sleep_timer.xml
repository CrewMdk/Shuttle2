    <string name="sleep_timer_play_to_track_end">Speel het laatste nummer om te eindigen</string>
    <!-- Button to close the sleep timer dialog -->
    <string name="sleep_timer_dialog_button_close">Sluiten</string>
    <!-- Button to stop the sleep timer -->
    <string name="sleep_timer_dialog_button_stop_timer">Stop de timer</string>
    <!-- Button to set the time for the sleep timer -->
    <string name="sleep_timer_dialog_button_set_time">Tijd instellen</string>
