<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- The title of the menu button. When clicked, will show the 'free trial' dialog, with an option for the user to upgrade-->
    <string name="menu_trial">Пробная версия</string>
    <!-- The heading of the 'free trial' dialog -->
    <string name="trial_heading_trial">Бесплатная пробная версия</string>
    <!-- Message shown when user is starting a trial. Note: This is meant to be friendly/fun! It doesn't have to say 'until you lose your mind' -->
    <string name="trial_description_trial">После пробной версии скорость воспроизведения будет немного увеличиваться каждый день, пока не достигнет умопомрачительных величин.</string>
    <!-- Shows how many days are left in the free trial-->
    <plurals name="trial_days_remaining">
        <item quantity="one">Остался 
      <xliff:g example="1" id="count">{count}</xliff:g>
       день</item>
        <item quantity="other">Осталось 
      <xliff:g example="30" id="count">{count}</xliff:g>
       дн.</item>
        <item quantity="few">Осталось 
      <xliff:g example="30" id="count">{count}</xliff:g>
       дн.</item>
        <item quantity="many">Осталось 
      <xliff:g example="30" id="count">{count}</xliff:g>
       дн.</item>
    </plurals>
    <!-- Shown when the user's trial has expired -->
    <string name="trial_heading_expired">Срок действия пробной версии истек</string>
    <!-- Message shown when trial has expired. Note: This is meant to be friendly/fun! It doesn't have to say 'until you lose your mind' -->
    <string name="trial_description_expired">Скорость воспроизведения будет немного увеличиваться каждый день, пока не достигнет умопомрачительных величин.</string>
    <!-- When a trial ends, the playback speed is increased by a small amount each day. This text shows the current playback speed -->
    <string name="trial_playback_speed">Скорость воспроизведения: 
    <xliff:g example="1.4x" id="speed">{speed}</xliff:g></string>
    <!-- Button to upgrade from the trial version to a paid version -->
    <string name="trial_button_upgrade">Обновление</string>
    <!-- Title of dialog shown after user has completed a purchase-->
    <string name="dialog_title_purchase_complete">Спасибо</string>
    <!-- Message shown in dialog after user has completed a purchase. Note: This is informal, but should reflect sincere appreciation (not just a generic 'thank you')-->
    <string name="dialog_message_purchase_complete">Вы успешно обновились до полной версии S2. Большое спасибо за поддержку приложения.</string>
</resources>
