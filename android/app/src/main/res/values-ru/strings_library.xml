<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title for the list of artists -->
    <string name="artists">Исполнители</string>
    <!-- Title for the list of albums -->
    <string name="albums">Альбомы</string>
    <plurals name="albumsPlural">
        <item quantity="one"><xliff:g example="1" id="count">{count}</xliff:g>
       альбом</item>
        <item quantity="other"><xliff:g example="5" id="count">{count}</xliff:g>
       альбомы</item>
        <item quantity="few"><xliff:g example="5" id="count">{count}</xliff:g>
       альбома</item>
        <item quantity="many"><xliff:g example="5" id="count">{count}</xliff:g>
       альбомов</item>
    </plurals>
    <!-- Title for the list of songs -->
    <string name="songs">Песни</string>
    <!-- The number of songs for an album/playlist/genre -->
    <plurals name="songsPlural">
        <item quantity="one"><xliff:g example="1" id="count">{count}</xliff:g>
       песня</item>
        <item quantity="other"><xliff:g example="12" id="count">{count}</xliff:g>
       песни</item>
        <item quantity="few"><xliff:g example="12" id="count">{count}</xliff:g>
       песни</item>
        <item quantity="many"><xliff:g example="12" id="count">{count}</xliff:g>
       песен</item>
    </plurals>
    <!-- Title for the list of genres -->
    <string name="genres">Жанры</string>
    <!-- Title for the list of playlists -->
    <string name="library_playlists">Плейлисты</string>
    <!-- Shown when the app is scanning for songs in the library -->
    <string name="library_scan_in_progress">Сканирование вашей библиотеки</string>
    <!-- Shown to indicate the number of tracks/albums/artists which have been selected, in multi-selection mode-->
    <plurals name="multi_select_items_selected">
        <item quantity="one">Выбрано 
      <xliff:g example="1" id="count">{count}</xliff:g></item>
        <item quantity="other">Выбрано 
      <xliff:g example="3" id="count">{count}</xliff:g></item>
        <item quantity="few">Выбрано 
      <xliff:g example="3" id="count">{count}</xliff:g></item>
        <item quantity="many">Выбрано 
      <xliff:g example="3" id="count">{count}</xliff:g></item>
    </plurals>
    <!-- Shown on the library screen, if the user has manually deselected all the library tabs in appearance settings-->
</resources>
