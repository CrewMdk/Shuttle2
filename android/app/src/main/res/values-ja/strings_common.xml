<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Shown when text is required in a text field -->
    <string name="validation_field_required">必須</string>
    <!-- Shown when an unexpected error occurs -->
    <string name="error_unexpected">予期しないエラーが発生しました</string>
    <!-- Shown when an unknown error occurs -->
    <string name="error_unknown">不明なエラーが発生しました</string>
    <!-- Shown when items (an album, artist or song) are added to the queue -->
    <string name="queue_item_added"><xliff:g example="Lateralus" id="item_name">{item_name}</xliff:g> がキューに追加されました</string>
    <!-- Shown when songs are added to the queue -->
    <plurals name="queue_songs_added">
        <item quantity="other"><xliff:g example="30" id="count">{count}</xliff:g> 曲がキューに追加されました</item>
    </plurals>
    <!-- Shown when albums are added to the queue -->
    <plurals name="queue_albums_added">
        <item quantity="other"><xliff:g example="30" id="count">{count}</xliff:g> アルバムがキューに追加されました</item>
    </plurals>
    <!-- Shown when artists are added to the queue -->
    <plurals name="queue_artists_added">
        <item quantity="other"><xliff:g example="30" id="count">{count}</xliff:g> アーティストがキューに追加されました</item>
    </plurals>
    <!-- Shown when current playback queue is added to a playlist -->
    <string name="playlist_queue_added">キューが <xliff:g example="Favourites" id="playlist_name">{playlist_name}</xliff:g> に追加されました</string>
    <!-- Shown when songs are added to a playlist -->
    <plurals name="playlist_songs_added">
        <item quantity="other"><xliff:g example="30" id="count">{count}</xliff:g> 曲が <xliff:g example="Favourites" id="playlist_name">{playlist_name}</xliff:g> に追加されました</item>
    </plurals>
    <!-- Shown when albums are added to a playlist -->
    <plurals name="playlist_albums_added">
        <item quantity="other"><xliff:g example="30" id="count">{count}</xliff:g> アルバムが <xliff:g example="Favourites" id="playlist_name">{playlist_name}</xliff:g> に追加されました</item>
    </plurals>
    <!-- Shown when artists are added to a playlist -->
    <plurals name="playlist_artists_added">
        <item quantity="other"><xliff:g example="30" id="count">{count}</xliff:g>
      アーティストが
      <xliff:g example="Favourites" id="playlist_name">{playlist_name}</xliff:g>
      に追加されました</item>
    </plurals>
    <!-- Shown when genres are added to a playlist -->
    <plurals name="playlist_genres_added">
        <item quantity="other"><xliff:g example="30" id="count">{count}</xliff:g>
      ジャンルが
      <xliff:g example="Favourites" id="playlist_name">{playlist_name}</xliff:g>
      に追加されました</item>
    </plurals>
    <!-- Shown when a song fails to be deleted -->
    <string name="delete_song_failed">曲を削除できませんでした</string>
    <!-- The disc number for this album-->
    <string name="disc_number">ディスク
    <xliff:g example="1" id="disc_number">{disc_number}</xliff:g></string>
    <!-- Shown when the songs list finishes loading, and there are no songs-->
    <string name="song_list_empty">曲はありません</string>
    <!-- Shown when the albums list finishes loading, and there are no songs-->
    <string name="album_list_empty">アルバムはありません</string>
    <!-- Shown when the artists list finishes loading, and there are no songs-->
    <string name="artist_list_empty">アーティストはありません</string>
    <!-- Shown when the genres list finishes loading, and there are no songs-->
    <string name="genre_list_empty">ジャンルはありません</string>
    <!-- Shown when the playlists list finishes loading, and there are no songs-->
    <string name="playlist_list_empty">プレイリストはありません</string>
    <!-- Shown when a playlist is empty -->
    <string name="playlist_empty">プレイリストが空です</string>
    <!-- Button to toggle the 'repeat' mode-->
    <string name="button_repeat">リピート</string>
    <!-- Button to indicate 'seek forward' (skipping forward by, say, 10 seconds)-->
    <string name="button_seek_forward">早送り</string>
    <!-- Button to skip to the next track -->
    <string name="button_skip_next">次の曲</string>
    <!-- Button to play/pause the current song-->
    <string name="button_toggle_playback">再生/一時停止</string>
    <!-- Button to indicate 'seek backward' (skipping backwards by, say, 10 seconds)-->
    <string name="button_seek_backward">巻戻し</string>
    <!-- Button to skip to the previous track-->
    <string name="button_skip_previous">前の曲</string>
    <!-- Button to shuffle songs-->
    <string name="button_content_description_shuffle">シャッフル</string>
    <!-- Button to shuffle songs using 'album shuffle' mode (shuffles albums, but plays songs in track order)-->
    <string name="button_album_shuffle">アルバムシャッフル</string>
    <!-- 'Loading' text. Shown on various screens as their content is loading-->
    <string name="loading">読込中…</string>
    <!-- Shown when the playback queue is empty -->
    <string name="queue_empty">キューが空です</string>
</resources>
