    <string name="media_provider_dialog_remove_title"><xliff:g example="Jellyfin" id="provider_type">{provider_type}</xliff:g>
     निकालें</string>
    <!-- Warning when user attempts to remove a media provider, that their media associated with that provider will be deleted. A media provider is a source of media, e.g. Android Media Store, S2 Media Provider, Jellyfin Media Server, etc.-->
    <string name="media_provider_dialog_remove_subtitle">यह 
    <xliff:g example="Jellyfin" id="provider_type">{provider_type}</xliff:g>
