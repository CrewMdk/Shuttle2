    <string name="media_provider_description_plex">व्यक्तिगत मीडिया सर्वर</string>
    <!-- Shown when a media provider is being scanned. Indicates the current progress of that scan -->
    <string name="media_provider_scan_progress"><xliff:g example="5" id="progress">{progress}</xliff:g>
    /
    <xliff:g example="25" id="total">{total}</xliff:g>
     स्कैन किए गए गाने</string>
    <!-- Shown when a media provider scan has completed -->
    <string name="media_provider_scan_success_subtitle"><xliff:g example="5" id="inserts">{inserts}</xliff:g>
     आवेषण,
    <xliff:g example="15" id="updates">{updates}</xliff:g>
     अपडेट,
    <xliff:g example="25" id="deletes">{deletes}</xliff:g>
     हटाता है</string>
    <!-- Shown when a media provider scan has failed -->
    <string name="media_provider_scan_failure_title">स्कैन विफल रहा</string>
